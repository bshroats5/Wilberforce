import matplotlib.pyplot as plt

# Example data for teams
teams = ['Wilberforce Bulldogs', 'Eastern Kentucky Colonels']
fouls = [34, 22]

# Create a bar graph
plt.bar(teams, fouls)

# Define colors for each team
wilberforce_color = 'green'
eastern_kentucky_color = 'maroon'

# Create a bar graph with custom colors
plt.bar(teams, fouls, color=[wilberforce_color, eastern_kentucky_color])


# Add title and labels
plt.title('Team Fouls')
plt.xlabel('Teams')
plt.ylabel('Fouls')

# Show the plot
plt.show()
